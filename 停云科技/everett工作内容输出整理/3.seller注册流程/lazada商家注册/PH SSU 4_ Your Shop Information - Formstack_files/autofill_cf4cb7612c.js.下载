Formstack.AutoFill=function(a,b){a=a+"/admin/plugin/hook/autofill/"+b+"/check/3?ajax=1",_field=null,_total_field=null,_used={},_form=b,this.addGroup=function(a,b){var c=document.createElement("input");c.id="autofill"+b[0].to,c.type="checkbox";var d=this;c.onclick=function(){this.checked&&d._doFill(b)};var e=document.createElement("div"),f=document.createElement("label");f.setAttribute("for",c.id),f.innerHTML=a,f.style.marginBottom="10px",e.appendChild(c),e.appendChild(f);var g=document.getElementById("label"+b[0].to);if(!g){var h="field"+b[0].to;g=jQuery("[id^="+h+"]")[0],g&&g.id===h+"-1-1"?g=jQuery("[id^=matrix-"+h+"]")[0]:g&&jQuery("#signature"+b[0].to).length&&(g=null)}jQuery(e).width("auto").height(30).insertBefore(g)},this._doFill=function(a){for(i=0;i<a.length;i++){var b=window["form"+_form];b.copyFieldValue(a[i].from,a[i].to)}}};